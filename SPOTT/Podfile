# Uncomment this line to define a global platform for your project
# platform :ios, '8.0'
# Uncomment this line if you're using Swift
# use_frameworks!


# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY START - can be moved as a block
def j2objc_shared
    pod 'j2objc-shared-debug', :configuration => ['Debug'], :path => '../spottshared/build/j2objcOutputs'
    pod 'j2objc-shared-release', :configuration => ['Release'], :path => '../spottshared/build/j2objcOutputs'
end
# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY END

target 'SPOTT' do
    platform :ios, '8.3'
    j2objc_shared

end

target 'SPOTTTests' do
    platform :ios, '8.3'
    j2objc_shared

end
